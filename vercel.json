{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "next dev",
  "installCommand": "npm ci --no-optional --prefer-offline",
  "framework": "nextjs",
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true,
      "preview": true
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "20.x",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "api/*": {
      "memory": 1024
    }
  }
} 